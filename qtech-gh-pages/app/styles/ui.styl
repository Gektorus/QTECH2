body {
	background-color: #edd9d3;
}

.ui_holder {
	width: 900px;
	margin: 20px auto;
}

.ui-item {
	margin-top: 10px;
}

.ui-item.overflow {
	overflow: auto;
	padding: 10px 0;
}

ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

.checkbox-large_label, .checkbox-small_label, .checkbox-medium_label {
	border: 1px solid;
	border-radius: 4px;
	box-shadow: 0 0 .5px #b63f56;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-large, .checkbox-small, .checkbox-medium, .itempick_radio {
	position: absolute;
	left: -9999px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-large_slider:after, .checkbox-small_slider:after, .checkbox-medium_slider:after {
	content: '';
	position: absolute;
	left: 0;
	top: 0;
	-webkit-transition: left ease-in .4s;
	transition: left ease-in .4s;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-large:checked ~ .checkbox-large_label .checkbox-large_value:after {
	left: -100%;
}

.checkbox-large:checked ~ .checkbox-large_label .checkbox-large_value:first-child {
	color: #fff;
}

.checkbox-large:checked ~ .checkbox-large_label .checkbox-large_value:last-child {
	color: #b63f56;
}

.checkbox-large_label .checkbox-large_value:last-child {
	color: #fff;
}

.checkbox-large_label {
	display: table;
	color: #b63f56;
	width: 859px;
	height: 73px;
	padding: 5px;
	font-size: 28px;
	font-weight: 300;

	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-large_value {
	display: table-cell;
	width: 50%;
	height: 100%;
	text-align: center;
	vertical-align: middle;
	-webkit-transition: color ease-in-out .4s;
	transition: color ease-in-out .4s;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-large_slider {
	position: relative;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-large_slider:after {
	width: 100%;
	height: 100%;
	background-color: #b63f56;
	z-index: -1;
	border-radius: 7px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small:checked ~ .checkbox-small_label .checkbox-small_slider:after {
	left: 46px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small:checked ~ .checkbox-small_label .checkbox-small_value:after {
	content: 'on';
	left: -8px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small_label {
	color: #b63f56;
	display: inline-block;
	width: 81px;
	height: 30px;
	padding: 5px;
	position: relative;

	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small_slider {
	position: relative;
	display: inline-block;
	width: 100%;
	height: 100%;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small_slider:after {
	width: 20px;
	height: 20px;
	background-color: #b63f56;
	border-radius: 7px;
	top: 50%;
	margin-top: -10px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small_value {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-small_value:after {
	content: 'off';
	position: absolute;
	display: inline-block;
	left: 8px;
	width: 100%;
	height: 100%;
	font-size: 15px;
	font-weight: 400;
	text-transform: uppercase;
	text-align: center;
	vertical-align: middle;
	top: 42%;
	margin-top: -.5em;
	opacity 0
	outline: 10px solid transparent; // prevent antialiasing bug
	/*-webkit-transition: left ease-in .4s;*/
	/*transition: left ease-in .4s;*/
}

.checkbox-medium:checked ~ .checkbox-medium_label .checkbox-medium_slider:after {
	left: 52px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium:checked ~ .checkbox-medium_label .checkbox-medium_value:after {
	content: 'on';
	left: -20px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium_label {
	display: inline-block;
	color: #fff;
	width: 100px;
	height: 40px;
	padding: 5px;
	position: relative;

	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium_slider {
	position: relative;
	display: inline-block;
	width: 100%;
	height: 100%;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium_slider:after {
	width: 28px;
	height: 28px;
	background-color: #fff;
	border-radius: 7px;
	left: 5px;
	top: 50%;
	margin-top: -14px;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium_value {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium_value:after {
	content: 'off';
	position: absolute;
	left: 20px;
	width: 100%;
	height: 100%;
	font-size: 20px;
	top: 50%;
	margin-top: -.5em;
	font-weight: 400;
	text-transform: uppercase;
	text-align: center;
	opacity 0
	outline: 10px solid transparent; // prevent antialiasing bug
	/*-webkit-transition: left ease-in .4s;*/
	/*transition: left ease-in .4s;*/
}

.checkbox-large_label:hover,
.checkbox-small_label:hover,
.checkbox-medium_label:hover {
	background-color: rgba(0, 0, 0, 0.01);
	cursor: pointer;
}

.button {
	display: table-cell;
	text-transform: uppercase;
	text-decoration: none;
	vertical-align: middle;
	color: inherit;
	border: none;
	background-color: transparent;
	padding: 0;
	margin: 0;
	border: 1px solid;
	white-space: nowrap;
	-webkit-transition: background-color .3s;
	transition: background-color .3s;
	box-shadow: 0 0 .5px #b63f56;
}

.button:focus {
	outline: none;
}

.button__cherry {
	background-color: #ae2b44;
	height: 70px;
	padding: 0 60px;
	font-size: 26px;
	color: #fff;
	font-weight: 300;
	border-radius: 9px;
	border: none;
}

.button__cherry:hover {
	background-color: #ce4560;
}

.button__ghost {
	background-color: transparent;
	color: #ae2b44;
	padding: 0 40px;
	height: 67px;
	text-align: center;
	border-radius: 7px;
	font-weight: normal;
	font-size: 26px;
}

.button__ghost:hover {
	background-color: rgba(0, 0, 0, 0.02);
}

.button__action {
	color: #fff;
	min-width: 209px;
	height: 66px;
	text-align: center;
	border-radius: 6px;
	font-size: 28px;
	font-weight: 300;
	padding: 0 20px;
}

.button__action:hover {
	background-color: rgba(0, 0, 0, 0.02);
}

.button__action:before {
	content: '';
	display: inline-block;
	-webkit-transform: translate(-20px, -2px);
	-ms-transform: translate(-20px, -2px);
	transform: translate(-20px, -2px);
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 8.5px 10px 8.5px 0;
	border-color: transparent #fff transparent transparent;
}

.button__text {
	font-size: 20px;
	border: none;
	font-weight: 400;
	color: #b63f56;
}

.button__text:hover {
	color: #ce4560;
}

.tab {
	background-color: #6b6261;
	display: table-cell;
	height: 137px;
	padding: 0 20px;
	color: white;
	text-align: center;
	vertical-align: middle;
	font-size: 30px;
	font-weight: 300;
	position: relative;
	-webkit-transition: background-color .1s;
	transition: background-color .1s;
}

.tab:hover {
	background-color: #736968;
	cursor: pointer;
}

.tab__active {
	background-color: #faf4f2;
	color: #91767b;
}

.tab__active:hover {
	background-color: #f9f1ef;
}

.tab_section {
	display: table;
	table-layout: fixed;
}

.tab_section {
	width: 100%;
}

.tab_section.items-1 .tab, .tab_section.items-1 .accordion {
	width: 100%;
}

.tab_section.items-2 .tab, .tab_section.items-2 .accordion {
	width: 50%;
}

.tab_section.items-3 .tab, .tab_section.items-3 .accordion {
	width: 33.33333%;
}

.tab_section.items-4 .tab, .tab_section.items-4 .accordion {
	width: 25%;
}

.tab_section.items-5 .tab, .tab_section.items-5 .accordion {
	width: 20%;
}

.tab_section.items-6 .tab, .tab_section.items-6 .accordion {
	width: 16.66667%;
}

.jelect {
	position: relative;
	display: inline-block;
	vertical-align: top;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	width: 417px;
	height: 70px;
	background: transparent;
	cursor: pointer;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

.jelect:focus,
.jelect *:focus {
	outline: none;
}

.jelect-input {
	display: none;
}

.jelect-current {
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden;
	padding: 0 58px 0 40px;
	font-family: 'Roboto', sans-serif;
	font-size: 29.5px;
	color: #8b6e74;
	line-height: 70px;
	border: 1px solid #91767b;
	border-radius: 2px;
	overflow-wrap: normal;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.jelect-current:focus {
	outline: none;
}

.jelect-current:before {
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	margin: auto;
	display: block;
	width: 16px;
	height: 16px;
	content: '';
}

.jelect-current:after {
	position: absolute;
	top: 0;
	right: 0;
	bottom: -2px;
	width: 70px;
	height: 100%;
	border-left: 1px solid #91767b;
	margin: auto;
	background: url(../images/jelect-dd.png) center center no-repeat;
	content: '';
}

.jelect-options {
	position: absolute;
	top: 100%;
	left: 0;
	right: 0;
	margin-top: -2px;
	display: none;
	max-height: 800px;
	overflow-y: auto;
	overflow-x: hidden;
	background: #fff;
	border: 1px solid #91767b;
	border-radius: 2px;
	padding-left: 0;
}

.jelect-options_state_active {
	display: block;
}

.jelect-option {
	position: relative;
	overflow: hidden;
	padding-left: 31px;
	display: block;
	height: 38px;
	font-family: 'Roboto', sans-serif;
	font-size: 18px;
	line-height: 71px;
	height: 73px;
	color: #8b6e74;
	font-weight: 400;
	background-color: #eaeaea;
	border-bottom: 1px solid #bdacaf;
	overflow-wrap: normal;
	text-overflow: ellipsis;
	white-space: nowrap;
	-webkit-transition: 0.125s ease-out;
	transition: 0.125s ease-out;
}

.jelect-option:first-child {
	border-top: 1px solid #bdacaf;
}

.jelect-option:last-child {
	margin-bottom: 0;
	-webkit-box-shadow: none;
	box-shadow: none;
	border-bottom: none;
}

.jelect-option:hover,
.jelect-option_state_active {
	background-color: #e9e2e3;
}

.jelect-option:before {
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	margin: auto;
	display: block;
	width: 16px;
	height: 16px;
	background-repeat: no-repeat;
	content: '';
}

.jelect__language {
	height: 100%;
	max-height: 136px;
	width: 300px;
	background-color: rgba(65, 49, 37, 0.95);
}

.jelect__language .jelect-current {
	border: none;
	color: inherit;
}

.jelect__language .jelect-current:before {
	display: none;
}

.jelect__language .jelect-current:after {
	right: 46px;
	width: 0;
	height: 0;
	background: none;
	border-style: solid;
	border-width: 3px 4px 0 4px;
	border-color: #fff transparent transparent transparent;
}

.jelect__language .jelect-options {
	border: none;
	border-radius: 0;
}

.jelect__language .jelect-option {
	background-color: rgba(65, 49, 37, 0.95);
	color: #fff;
}

.jelect__language .jelect-option:hover {
	background-color: rgba(78, 59, 44, 0.95);
}

.jelect__small {
	width: 80px;
	height: 40px;
}

.jelect__small .jelect-current {
	border-radius: 0;
	height: 39px;
	font-size: 18px;
	padding: 0;
	line-height: 39px;
	color: #c14d64;
	font-weight: 400;
	padding-left: 15px;
	border: none;
}

.jelect__small .jelect-current:after {
	display: inline-block;
	background: none;
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 7px 0 7px 6px;
	border-color: transparent transparent transparent #c14d64;
	position: relative;
	margin-left: 10px;
}

.jelect__small .jelect-options {
	border-top: none;
	margin-top: -4px;
}

.jelect__small .jelect-option {
	padding-left: 15px;
	height: 40px;
	line-height: 40px;
}

.input-text {
	width: 260px;
	height: 46px;
	border: 1px solid #91767b;
	border-radius: 2px;
	background-color: transparent;
	color: #91767b;
	font-size: 24px;
	font-family: inherit;
	font-weight: 400;
	padding: 0 20px;
	-webkit-transition: border-color .4s;
	transition: border-color .4s;
}

.input-text:hover {
	border-color: #706364;
}

.input-text:focus {
	outline: none;
	border-color: #ae2b44;
}

.input-text__invalid {
	-webkit-box-shadow: 0 0 6px #b63f56;
	box-shadow: 0 0 6px #b63f56;
	border-color: #ae2b44;
}

.input-field {
	position: relative;
	display: inline-block;
}

.input_hint {
	position: absolute;
	display: inline-block;
	left: 260px;
	top: 0;
	margin-left: 20px;
	font-size: 24px;
	width: auto;
	height: 46px;
	padding: 0 60px 0 34px;
	font-weight: 300;
	line-height: 46px;
	color: #fff;
	background-color: #6a6161;
	border-radius: 4px;
	white-space: nowrap;
}

.input_hint:before {
	content: '';
	position: absolute;
	top: 50%;
	margin-top: -10px;
	left: -12px;
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 10px 12px 10px 0;
	border-color: transparent #6a6161 transparent transparent;
}

.input_hint .help {
	position: absolute;
	right: 0;
	top: 0;
	display: inline-block;
	height: 100%;
	padding: 0 25px;
	background: url(../images/input__help.png) center center no-repeat;
	-webkit-background-size: 10px 20px;
	background-size: 10px 20px;
	border-left: 2px solid rgba(0, 0, 0, 0.3);
	border-top-right-radius: 4px;
	border-bottom-right-radius: 4px;
}

.input_hint .help:hover {
	cursor: pointer;
	background-color: #857979;
}

.input_hint .help:hover .help_text {
	left: -50%;
	opacity: 1;
}

.input_hint .help_text {
	position: absolute;
	font-size: 18px;
	font-weight: 400;
	padding: 5px;
	border-radius: 4px;
	width: 200px;
	top: -100%;
	margin-top: -25px;
	left: -9999px;
	margin-left: -50px;
	background-color: #6a6161;
	color: #fff;
	text-align: center;
	opacity: 0;
	-webkit-transition: opacity .4s;
	transition: opacity .4s;
}

.input_hint .help_text:after {
	content: '';
	position: absolute;
	left: 50%;
	margin-left: -5px;
	bottom: -6px;
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 6px 5px 0 5px;
	border-color: #6a6161 transparent transparent transparent;
}

.input_hint__error {
	background-color: #ae2b44;
	color: #fff;
}

.input_hint__error:before {
	border-color: transparent #b63f56 transparent transparent;
}

.input_hint__error .help {
	border-color: #e9c6cd;
}

.input_hint__error .help:hover {
	background-color: #c86074;
}

.input_hint__error .help_text {
	background-color: #ae2b44;
	color: #fff;
}

.input_hint__error .help_text:after {
	border-color: #b63f56 transparent transparent transparent;
}

.time-table {
	width: 100%;
	overflow: auto;
}

.time-table_section {
	float: left;
}

.section__days {
	width: 38%;
	padding-top: 80px;
	min-width: 225px;
	background-color: #bdb2b8;
}

.section__time {
	width: 62%;
	font-size: 20px;
	font-weight: 400;
}

.time_holder {
	padding-top: 50px;
	background-color: #bdb2b8;
	overflow: auto;
}

.time_holder .time_value:first-child {
	text-indent: 0;
}

.time_value {
	text-indent: -.5px;
	display: inline-block;
	position: relative;
	z-index: 1;
	color: #977f85;
}

.days_name {
	display: block;
	font-size: 28px;
	font-weight: 400;
	padding: 5px 40px 5px 25px;
	color: #8e7278;
}

.days_row .pick:last-child {
	border-right: 2px solid #dac5c1;
}

.days_row .pick:first-child {
	border-left: none;
}

.pick {
	float: left;
	width: 4.166666666666667%;
	height: 43px;
	border-bottom: 2px solid #dac5c1;
	border-left: 2px solid #dac5c1;
	position: relative;
	-webkit-transition: background-color .4s;
	transition: background-color .4s;
}

.pick.highlighted {
	background-color: #b63f56;
}

.pick:hover {
	cursor: pointer;
}

.pickable-table .days_row:first-child .pick:before {
	content: '';
	position: absolute;
	height: 25px;
	top: -25px;
	left: -2px;
	width: 2px;
	background-color: #dac5c1;
}

.pickable-table .days_row:first-child .pick:first-child:before {
	display: none;
}

.time-table__fixed {
	width: 847px;
	height: 352px;
	overflow: visible;
}

.time-table__fixed .time_holder {
	padding-top: 40px;
}

.time-table__fixed .section__days {
	width: 38%;
	padding-top: 70px;
	min-width: 225px;
	background-color: #bdb2b8;
}

.time-table__fixed .days_name {
	font-size: 24px;
	padding-bottom: 4px;
}

.time-table__fixed .pick {
	height: 38px;
}

.time-table__fixed .time_value {
	font-size: 18px;
	text-indent: 0;
}

.popup_wrapper {
	min-width: 400px;
	min-height: 400px;
	position: fixed;
	top: 150%;
	left: 50%;
	z-index: 11;
	background-color: #000;
	display: none;
	-webkit-transition: top .4s;
	transition: top .4s;
}

.popup_overlay {
	z-index: 10;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.6);
	display: none;
}

.popup_overlay:hover {
	cursor: pointer;
}

.popup__visible {
	display: block;
}

.itempick {
	color: #b63f56;
	border: 1px solid;
	border-radius: 4px;
	box-shadow: 0 0 .5px #b63f56;
	height: 73px;
	width: 100%;
	padding: 5px;

	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.itempick_holder {
	/*overflow: auto;*/
	width: 100%;
	height: 100%;
	position: relative;
	overflow: hidden
}

.itempick_label {
	float: left;
	height: 100%;
	text-align: center;
	position: relative;
	-webkit-transition: color .4s;
	transition: color .4s;
	z-index: 2;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.itempick_label:hover {
	cursor: pointer;
}

.itempick_slider {
	position: absolute;
	height: 100%;
	background-color: #ae2b44;
	border-radius: 3px;
	left: 0;
	top: 0px;
	z-index: 1;
	-webkit-transition: left .4s;
	transition: left .4s;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.itempick_value {
	font-size: 28px;
	font-weight: 400;
	position: relative;
	display: block;
	margin-top: -.5em;
	top: 50%;
	outline: 10px solid transparent; // prevent antialiasing bug
}

.items-1 .itempick_label, .items-1 .itempick_slider {
	width: 100%;
}

.items-1 .itempick_radio:nth-of-type(1):checked ~ .itempick_slider {
	left: 0%;
}

.items-1 .itempick_radio:nth-of-type(1):checked + .itempick_label {
	color: #fff;
	pointer-events: none;
}

.items-2 .itempick_label, .items-2 .itempick_slider {
	width: 50%;
}

.items-2 .itempick_radio:nth-of-type(1):checked ~ .itempick_slider {
