.accordion {
	background-color: #6a6161;
	position: relative;
	color: #fff;
	font-weight: 200;
	height: 130px;
	width: 100%;
	padding-left: 80px;
	letter-spacing: -0.8px;
	word-spacing: 1px;
	-webkit-transition: background-color .3s;
	transition: background-color .3s;
}

.accordion:hover {
	cursor: pointer;
	background-color: #4f4949;
}

.accordion_name {
	font-size: 36px;
	position: relative;
	display: inline-block;
	top: 50%;
	width: 100%;
	margin-top: -.5em;
}

.accordion_name:after {
	content: '';
	position: absolute;
	right: 40px;
	top: 50%;
	margin-top: -3px;
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 6px 7px 0 7px;
	border-color: #fff transparent transparent transparent;
	-webkit-transition: border-width .4s;
	transition: border-width .4s;
}

.accordion.accordion__visible:after {
	border-width: 0 3px 3px 3px;
	border-color: transparent transparent #fff transparent;
}

.tab_section .accordion {
	display: table-cell;
	padding-left: 0;
	text-align: center;
	height: 150px;
}

.tab_section .accordion_name {
	font-size: 26px;
	padding: 0 10px;
}

.tab_section .accordion_name:after {
	top: 120%;
	left: 50%;
}

.checkbox-small:checked ~ .checkbox-small_label.flipswitch-fade .checkbox-small_value:after, .flipswitch-fade .checkbox-small_value:after {
	-webkit-animation: fade 1s;
	animation: fade 1s;
	opacity 1
	outline: 10px solid transparent; // prevent antialiasing bug
}

.checkbox-medium:checked ~ .checkbox-medium_label.flipswitch-fade .checkbox-medium_value:after, .flipswitch-fade .checkbox-medium_value:after {
	-webkit-animation: fade 1s;
	animation: fade 1s;
	opacity 1
	outline: 10px solid transparent; // prevent antialiasing bug
}

.fixed-header_container {
	.checkbox-medium:checked ~ .checkbox-medium_label.flipswitch-fade .checkbox-medium_value:after, .flipswitch-fade .checkbox-medium_value:after {
		-webkit-animation: none
		animation: none
	}
}

.checkbox-large:checked ~ .checkbox-large_label.flipswitch-fade .checkbox-large_value:after, .flipswitch-fade .checkbox-large_value:after {
	-webkit-animation: fade 1s;
	animation: fade 1s;
	opacity 1
	outline: 10px solid transparent; // prevent antialiasing bug
}

@-webkit-keyframes fade {
	0% {
		opacity: 0;
	}
	25% {
		opacity: .25
	}
	50% {
		opacity: .75;
	}
	100% {
		opacity: 1;
	}
}

@keyframes fade {
	0% {
		opacity: 0;
	}
	25% {
		opacity: .25
	}
	50% {
		opacity: .75;
	}
	100% {
		opacity: 1;
	}
}
